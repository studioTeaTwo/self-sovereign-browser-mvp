{
  "name": "wallet",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm-run-all build:*",
    "build:webpack": "webpack --config webpack.config.js",
    "build:sass": "sass content/css/main.scss content/css/main.compiled.css",
    "watch": "npm-run-all -p watch:*",
    "watch:webpack": "npm run build:webpack -- --env development -w",
    "watch:sass": "chokidar \"content/panels/**/*.scss\" -c \"npm run build:sass\"",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "studioTeaTwo <studioTeaTwo@gmail.com> (https://studioteatwo.github.io/)",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "chokidar-cli": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "sass": "^1.69.7",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1"
  }
}
