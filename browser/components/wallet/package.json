{
  "name": "wallet",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm-run-all build:*",
    "build:webpack": "webpack --config webpack.config.js",
    "build:sass": "sass content/css/main.scss content/css/main.compiled.css",
    "watch": "npm-run-all -p watch:*",
    "watch:webpack": "npm run build:webpack -- --env development -w",
    "watch:sass": "chokidar \"content/panels/**/*.scss\" -c \"npm run build:sass\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "studioTeaTwo <studioTeaTwo@gmail.com> (https://studioteatwo.github.io/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "chokidar-cli": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.7",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
